{
  "name": "talendig-monorepo",
  "version": "1.0.0",
  "description": "Talendig NodeJS Learning Program - Monorepo",
  "private": true,
  "workspaces": [
    "NodeJS/1-class",
    "NodeJS/2-class",
    "NodeJS/3-class",
    "NodeJS/4-class",
    "NodeJS/5-class",
    "NodeJS/6-final-project"
  ],
  "scripts": {
    "start": "npm run start --workspace=NodeJS/1-class",
    "start:dev": "npm run start:dev --workspace=NodeJS/1-class",
    "start:qa": "npm run start:qa --workspace=NodeJS/1-class",
    "start:third": "npm run --workspace=NodeJS/3-class start:dev",
    "start:fourth": "npm run --workspace=NodeJS/4-class start:dev",
    "start:fifth": "npm run --workspace=NodeJS/5-class start:dev",
    "start:final": "npm run --workspace=NodeJS/6-final-project start:dev",
    "build": "npm run build --workspaces",
    "test": "npm run test --workspaces",
    "lint": "npm run lint --workspaces"
  },
  "keywords": [
    "nodejs",
    "learning",
    "talendig",
    "monorepo"
  ],
  "author": "Luis Adolfo Pimentel",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "2.8.19",
    "@types/express": "5.0.3",
    "@types/jsonwebtoken": "9.0.10",
    "@types/node": "^20.0.0",
    "nodemon": "^3.1.10",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "cors": "2.8.5",
    "dotenv": "17.2.3",
    "express": "5.1.0",
    "express-validator": "7.2.1",
    "firebase-admin": "13.5.0",
    "helmet": "8.1.0",
    "jsonwebtoken": "9.0.2",
    "pg": "8.16.3",
    "pg-hstore": "2.3.4",
    "sequelize": "6.37.7",
    "url": "0.11.4",
    "zod": "^3.22.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
